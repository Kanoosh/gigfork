<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
   <bean parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
      <property name="models">
         <list>
            <value>alfresco/model/gitHubPublishingModel.xml</value>
         </list>
      </property>
   </bean>

    <bean id="soundcloudChannelMessageBundle" class="org.alfresco.i18n.ResourceBundleBootstrapComponent">
        <property name="resourceBundles">
            <list>
                <value>org.alfresco.repo.publishing.github.gitHub-publishing</value>
            </list>
        </property>
    </bean>

   <bean id="gitHubDeliveryChannelType" class="org.alfresco.repo.publishing.github.GitHubChannelType" parent="baseChannelType">
      <property name="publishingHelper" ref="gitHubPublishingHelper" />
   </bean>

   <bean id="gitHubPublishingHelper" class="org.alfresco.repo.publishing.github.GitHubPublishingHelper">
      <property name="nodeService" ref="NodeService" />
      <property name="connectionFactory" ref="gitHubConnectionFactory" />
      <property name="encryptor" ref="metadataEncryptor" />
   </bean>

   <bean id="gitHubConnectionFactory" class="org.springframework.social.github.connect.GitHubConnectionFactory">
      <constructor-arg value="289427501067879" />
      <constructor-arg value="8680cd45e7024a5aa078034ddc2fbb09" />
   </bean>
</beans>
